<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="mvnSuite">
	<test name="AmazonLogin">
		<classes>
			<class name="Amazon.AmazonFromLogin">
				<methods>
					<include name="loginApp"></include>
					<include name="launchingBrowser"></include>
				</methods>

			</class>
		</classes>
	</test>

	<test name="DemowebshopLaunch">
		<classes>
			<class name="DemoWEbShop.DemoApp">
				<methods>
					<include name="loginDemoWebshop"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite> <!-- mvnSuite -->
